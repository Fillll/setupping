# OpenClaw image/runtime
OPENCLAW_IMAGE=ghcr.io/openclaw/openclaw:latest
OPENCLAW_CUSTOM_IMAGE=openclaw-with-vnstat:local
OPENCLAW_PORT=8081
TZ=Europe/London

# Telegram
TELEGRAM_BOT_TOKEN=replace_with_bot_token
# Comma-separated numeric IDs. Only these chats are served.
TELEGRAM_WHITELIST_CHAT_IDS=123456789,987654321
TELEGRAM_NON_WHITELIST_MODE=ignore

# Notes/RAG
LOGSEQ_PATH=/workspace/logseq
VECTOR_STORE_PATH=/workspace/vector
RAG_CITATIONS_REQUIRED=true
RAG_REINDEX_MODE=manual

# Browser automation
BROWSER_PROVIDER=browserless
BROWSER_WS_ENDPOINT=ws://chrome:3000
BROWSER_TIMEOUT_MS=120000
SCREENSHOT_PATH=/workspace/screenshots
SCREENSHOT_SEND_TO_TELEGRAM=true
VNSTAT_INTERFACE=eth0

# 1Password-assisted auth flow
OP_INTEGRATION_MODE=service-account
OP_SERVICE_ACCOUNT_TOKEN=replace_with_op_service_account_token
OP_VAULT=replace_with_vault_name

# Optional model/provider vars
# OPENAI_API_KEY=
# ANTHROPIC_API_KEY=
